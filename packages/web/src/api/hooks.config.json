  "parsedQueries": {
    "GetTeamQueueItems": {
      "parser": "parseGetTeamQueueItems",
      "parserModule": "@rediacc/shared/api/parsers/queue",
      "returnType": "QueueListResult",
      "queryKey": ["queue", "items"],
      "queryKeyParams": ["teamName"]
    },
    "GetQueueItemTrace": {
      "parser": "parseGetQueueItemTrace",
      "parserModule": "@rediacc/shared/api/parsers/queue",
      "returnType": "QueueTrace",
      "queryKey": ["queue", "trace"],
      "queryKeyParams": ["taskId"],
      "staleTime": 5000,
      "refetchInterval": 5000
    },
    "GetOrganizationDashboard": {
      "parser": "parseGetOrganizationDashboard",
      "parserModule": "@rediacc/shared/api/parsers/organization",
      "returnType": "OrganizationDashboardData",
      "queryKey": ["organization", "dashboard"],
      "staleTime": 60000
    },
    "GetAuditLogs": {
      "parser": "parseGetAuditLogs",
      "parserModule": "@rediacc/shared/api/parsers/audit",
      "returnType": "GetAuditLogs_ResultSet1[]",
      "queryKey": ["auditLogs"],
      "staleTime": 30000,
      "gcTime": 300000
    },
    "GetEntityAuditTrace": {
      "parser": "parseGetEntityAuditTrace",
      "parserModule": "@rediacc/shared/api/parsers/audit",
      "returnType": "AuditTraceResponse",
