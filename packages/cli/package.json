{
  "name": "@rediacc/cli",
  "version": "0.2.104",
  "description": "Rediacc CLI - Command line interface for Rediacc operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rdc": "./bin/rdc"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "bun:dev": "bun run src/index.ts",
    "bun:start": "bun run dist/index.js",
    "bun:test": "bun test",
    "build:exe": "bun build --compile --minify --sourcemap src/index.ts --outfile dist/rdc",
    "build:exe:win": "bun build --compile --minify --target=bun-windows-x64 src/index.ts --outfile dist/rdc-win.exe",
    "build:exe:mac": "bun build --compile --minify --target=bun-darwin-x64 src/index.ts --outfile dist/rdc-mac",
    "build:exe:mac-arm": "bun build --compile --minify --target=bun-darwin-arm64 src/index.ts --outfile dist/rdc-mac-arm",
    "build:exe:linux": "bun build --compile --minify --target=bun-linux-x64 src/index.ts --outfile dist/rdc-linux",
    "build:exe:all": "bun run build:exe:win && bun run build:exe:mac && bun run build:exe:mac-arm && bun run build:exe:linux"
  },
  "dependencies": {
    "@rediacc/shared": "^0.1.0",
    "axios": "^1.13.2",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "inquirer": "^13.0.2",
    "ora": "^9.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^24.10.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "bin"
  ],
  "keywords": [
    "rediacc",
    "cli",
    "devops",
    "automation"
  ],
  "author": "Rediacc",
  "license": "MIT"
}
