{
  "name": "@rediacc/cli",
  "version": "0.4.76",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:bundle": "node bundle.mjs",
    "build:sea": "node --experimental-sea-config sea-config.json",
    "clean": "rimraf dist",
    "dev": "tsx src/index.ts",
    "export:command-tree": "tsx scripts/export-command-tree.ts",
    "lint": "eslint src --ext .ts",
    "prepublishOnly": "npm run build && npm run build:bundle",
    "start": "node dist/index.js",
    "test": "playwright test --config tests/playwright.config.ts",
    "test:ceph": "playwright test --config tests/playwright.config.ts --project=ceph",
    "test:core": "playwright test --config tests/playwright.config.ts --project=core",
    "test:e2e": "playwright test --config tests/playwright.config.ts --project=e2e",
    "test:edition": "playwright test --config tests/playwright.config.ts --project=edition",
    "test:errors": "playwright test --config tests/playwright.config.ts --project=errors",
    "test:operations": "playwright test --config tests/playwright.config.ts --project=operations",
    "test:report": "playwright show-report tests/reports/cli",
    "test:resources": "playwright test --config tests/playwright.config.ts --project=resources",
    "test:run": "playwright test --config tests/playwright.config.ts",
    "test:security": "playwright test --config tests/playwright.config.ts --project=security",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:vscode": "playwright test --config tests/playwright.config.ts --project=vscode",
    "typecheck": "tsc --noEmit"
  },
  "author": "Rediacc",
  "bin": {
    "rdc": "./dist/cli-bundle.cjs"
  },
  "dependencies": {
    "@opentelemetry/context-zone": "^2.5.0",
    "@opentelemetry/core": "^2.5.0",
    "@opentelemetry/instrumentation": "^0.211.0",
    "@opentelemetry/instrumentation-fetch": "^0.211.0",
    "@opentelemetry/instrumentation-xml-http-request": "^0.211.0",
    "@opentelemetry/sdk-trace-web": "^2.5.0",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/runner": "^4.0.18",
    "@vitest/ui": "^4.0.18",
    "@vitest/utils": "^4.0.18",
    "inquirer": "^13.2.2",
    "react-i18next": "^16.5.4",
    "unist-util-visit": "^5.1.0",
    "zod": "^4.3.6"
  },
  "description": "Rediacc CLI - Command line interface for Rediacc operations",
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "@playwright/test": "^1.40.0",
    "@rediacc/shared": "*",
    "@rediacc/shared-desktop": "*",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.2",
    "axios": "^1.13.2",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.2",
    "dotenv": "^16.3.1",
    "esbuild": "^0.27.1",
    "execa": "^9.6.1",
    "i18next": "^25.7.2",
    "ora": "^9.1.0",
    "otpauth": "^9.4.1",
    "rimraf": "^6.1.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^4.0.16",
    "yaml": "^2.7.0"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist/cli-bundle.cjs"
  ],
  "keywords": [
    "automation",
    "cli",
    "devops",
    "rediacc"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts"
}
