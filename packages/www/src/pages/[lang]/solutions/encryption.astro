---
import SolutionPage from '../../../components/solution-pages/SolutionPage.astro';
import { SUPPORTED_LANGUAGES } from '../../../i18n/language-utils';
import type { Language } from '../../../i18n/types';

export function getStaticPaths() {
  return SUPPORTED_LANGUAGES.map((lang) => ({
    params: { lang },
    props: { lang },
  }));
}

const { lang } = Astro.props as { lang: Language };
---

<SolutionPage lang={lang} slug="encryption" />
