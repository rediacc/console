{
  "name": "@rediacc/cli",
  "version": "0.2.103",
  "description": "Rediacc CLI - Command line interface for Rediacc operations",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "rdc": "./bin/rdc"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist",
